<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Smoke cucumber tests" parallel="classes">
    <listeners>
        <listener class-name="cucumber.testng.hooks.CucumberHooks"></listener>
    </listeners>
    <test name="Cucumber Tests">
        <classes>
            <class name="cucumber.testng.runner.CucumberTestRunner"></class>
        </classes>
    </test>
</suite>